{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"],
        "functions": {
          "dist/controllers/*.js": {
            "maxDuration": 500
          },
          "dist/managers/*.js": {
            "maxDuration": 500
          },
          "dist/index.js": {
            "maxDuration": 500
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
